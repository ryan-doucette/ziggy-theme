{
  "name": "My Custom Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#0e0e0e", // Editor background color
    "editor.foreground": "#b29bff", // Editor text color
    "editor.lineHighlightBackground": "#262327", // Highlight current line
    "editorLineNumber.foreground": "#4d5267", // Line number color
    "editorLineNumber.activeForeground": "#b994f4", // Active line number color
    "editor.selectionBackground": "#3f3a40", // Selection highlight
    "editorGroupHeader.tabsBackground": "#171717",
    "icon.foreground": "#d1cffd",
    "window.activeBorder": "#ffffff8d", // Active window border color
    "window.inactiveBorder": "#4a4a4a", // Inactive window border color
    "tab.activeBackground": "#0e0e0e",
    "tab.inactiveBackground": "#171717",
    "tab.hoverBackground": "#212121",
    "tab.activeForeground": "#d1cffd",
    "tab.inactiveForeground": "#908fa9",
    "sideBar.background": "#0e0e0e", // Sidebar background color
    "sideBar.foreground": "#d1cffd", // Sidebar text color
    "sideBar.border": "#b994f46c",
    "sideBarSectionHeader.border": "#b994f46c",
    "sideBarTitle.foreground": "#b994f4", // Sidebar title color
    "sideBarSectionHeader.background": "#0e0e0e", // Sidebar section header background
    "activityBar.background": "#0e0e0e", // Activity bar background color
    "activityBar.foreground": "#d1cffd", // Activity bar text color
    "activityBar.border": "#b994f46c",
    "activityBarBadge.background": "#b994f4", // Activity bar badge color
    "activityBarBadge.foreground": "#0e0e0e",
    "statusBar.background": "#0e0e0e", // Status bar background color
    "statusBar.foreground": "#d1cffd", // Status bar text color
    "statusBar.noFolderBackground": "#0e0e0e", // Status bar no folder background
    "statusBar.border": "#b994f46c",
    "titleBar.activeBackground": "#0e0e0e", // Title bar active background
    "titleBar.activeForeground": "#d1cffd", // Title bar active text color
    "titleBar.inactiveBackground": "#0e0e0e", // Title bar inactive background
    "titleBar.inactiveForeground": "#d1cffd", // Title bar inactive text color
    "titleBar.border": "#b994f46c"
  },
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#999999",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type", "keyword.control"],
      "settings": {
        "foreground": "#8499d6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#b098fc"
      }
    },
    {
      "scope": ["variable", "identifier", "meta.definition.variable"],
      "settings": {
        "foreground": "#e2e2e2"
      }
    },
    {
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#eb9b9b"
      }
    },
    {
      "scope": ["constant.numeric", "number"],
      "settings": {
        "foreground": "#ff9f6b"
      }
    },
    {
      "scope": ["storage.modifier", "entity.name.type", "support.type"],
      "settings": {
        "foreground": "#e2d4e9"
      }
    },
    {
      "scope": ["constant", "support.constant", "constant.language"],
      "settings": {
        "foreground": "#c4a0ff"
      }
    },
    {
      "scope": ["statement", "keyword.return", "keyword.throw"],
      "settings": {
        "foreground": "#f288a9"
      }
    },
    {
      "scope": ["meta.preprocessor", "meta.directive"],
      "settings": {
        "foreground": "#fff371"
      }
    },
    {
      "scope": [
        "support.variable",
        "variable.language",
        "support.other.variable"
      ],
      "settings": {
        "foreground": "#fff371"
      }
    }
  ]
}
